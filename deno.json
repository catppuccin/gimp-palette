{
  "importMap": "./import_map.json",
  "fmt": {
    "include": ["**/*.ts"]
  },
  "lint": {
    "include": ["."],
    "exclude": ["dist/"]
  },
  "tasks": {
    "esm:add": "deno run -A https://esm.sh/v135 add",
    "esm:update": "deno run -A https://esm.sh/v135 update",
    "esm:remove": "deno run -A https://esm.sh/v135 remove",
    "npm": "deno run -A ./scripts/build_npm.ts",
    "palette": "deno run -A ./scripts/gen_palette.ts",
    "thirdparty": "deno run -A ./scripts/build_thirdparty.ts"
  },
  "version": "1.0.0"
}
